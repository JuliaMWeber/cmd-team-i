<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.fields</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <headline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.headline</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </headline>
                    <subHeadline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.subheadline</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </subHeadline>
                    <check>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:grid.bgcheck</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </check>
                    <accordion>
                        <title>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:gallery.title</title>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <container>
                                <type>array</type>
                                <title>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.addnew</title>
                                <el>
                                    <image>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.image</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>gif,jpg,jpeg,tif,tiff,bmp,pcx,tga,png,pdf,ai,svg</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>1</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>
                                            </config>
                                        </TCEforms>
                                    </image>
                                    <name>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.name</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                                <eval>trim,required</eval>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <!-- <firstname>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.firstname</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                                <eval>trim,required</eval>
                                            </config>
                                        </TCEforms>
                                    </firstname> -->
                                    <text>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.text</label>
                                            <config>
                                                <type>input</type>
                                                <cols>30</cols>
                                                <eval>trim,required</eval>
                                                <enableRichtext>1</enableRichtext>
                                            </config>
                                        </TCEforms>
                                    </text>
                                    <mail>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.mail</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                                <eval>trim</eval>
                                            </config>
                                        </TCEforms>
                                    </mail>
                                    <contact>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.contact</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                                <eval>trim</eval>
                                            </config>
                                        </TCEforms>
                                    </contact>
                                </el>
                            </container>
                        </el>
                    </accordion>
                    <otherText>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.text</label>
                            <config>
                                <type>text</type>
                                <rows>1</rows>
                                <cols>30</cols>
                                <eval>trim</eval>
                                <enableRichtext>0</enableRichtext>
                                <richtextConfiguration>default</richtextConfiguration>
                            </config>
                        </TCEforms>
                    </otherText>
                </el>
            </ROOT>
        </sDEF>
